#: src/options.cpp:133
#, c-format
msgid "Usage: taptempo [options]\n"
msgstr "Utilisation : taptempo [options]\n"

#: src/options.cpp:135
#, c-format
msgid "Options:\n"
msgstr "Options :\n"

#: src/options.cpp:136
#, c-format
msgid "  -h, --help\t\tdisplay this help message\n"
msgstr "  -h, --help\t\taffiche ce message d'aide\n"

#: src/options.cpp:137
#, c-format
msgid ""
"  -p, --precision\tset the decimal precision of the tempo display\n"
"\t\t\tdefault is %i digits, max is %i digits\n"
msgstr ""
"  -p, --precision\tchanger le nombre de décimale du tempo à afficher\n"
"\t\t\tla valeur par défaut est %i décimales, le max est %i décimales\n"

#: src/options.cpp:138
#, c-format
msgid ""
"  -r, --reset-time\tset the time in second to reset the computation\n"
"\t\t\tdefault is %i seconds\n"
msgstr ""
"  -r, --reset-time\tchanger le temps en seconde de remise à zéro du "
"calcul\n"
"\t\t\tla valeur par défaut est %i secondes\n"

#: src/options.cpp:139
#, c-format
msgid ""
"  -s, --sample-size\tset the number of samples needed to compute the "
"tempo\n"
"\t\t\tdefault is %i samples\n"
msgstr ""
"  -s, --sample-size\tchanger le nombre d'échantillons nécessaires au "
"calcul du tempo\n"
"\t\t\tla valeur par défaut est %i échantillons\n"

#: src/options.cpp:140
#, c-format
msgid "  -v, --version\t\tdisplay the version\n"
msgstr "  -v, --version\t\tafficher la version\n"

#: src/taptempo.cpp:69
#, c-format
msgid "Hit enter key for each beat (q to quit).\n"
msgstr "Appuyer sur la touche entrée en cadence (q pour quitter).\n"

#: src/taptempo.cpp:81
#, c-format
msgid "Bye Bye!\n"
msgstr "Au revoir !\n"

#: src/taptempo.cpp:107
#, c-format
msgid "Tempo: %s bpm\t"
msgstr "Tempo : %s bpm\t"

#: src/taptempo.cpp:112
#, c-format
msgid "[Hit enter key one more time to start bpm computation...]"
msgstr ""
"[Appuyer encore sur la touche entrée pour lancer le calcul du tempo...]"

